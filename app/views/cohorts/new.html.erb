<h1>New Cohort</h1>

<%= form_for @cohort do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <%= f.label :start %>
    <%= f.date_field :start %>
    <%= f.label :end %>
    <%= f.date_field :end %>
    <%= f.label :course_id %>
    <%= f.select "course_id", options_from_collection_for_select(Course.all, "id", "name")%>
    <%= f.submit %>
<% end %>
